<h1>All tests</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Quantity questions</th>
    <th>Actions</th>
  </tr>
  <% @tests.each do |test| %>
  <tr>
    <td><%= test.title %></td>
    <td><%= test.level %></td>
    <td><%= test.questions.size %></td>
    <td><%= link_to 'Show', test %></td>
  </tr>
  <% end %>
</table>

