<h1>Создать вопрос</h1>

<form action="/tests/1/questions" method="post">
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">

  <p>
    <label for="body">Введите вопрос:</label>
    <input type="text" name="question[body]" id="body">
  </p>

  <input name="question[test_id]" value="1" type="hidden">

  <input type="submit" name="Создать">
</form>
